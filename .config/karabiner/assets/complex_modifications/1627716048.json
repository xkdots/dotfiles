{
  "title": "Sinoridha Function Key",
  "rules": [
    {
      "description": "cmd + w",
      "manipulators": [{
        "from": {
          "key_code": "f1"
        },
        "to": [{
          "key_code": "w",
          "modifiers": [
            "command"
          ]
        }],
        "type": "basic"
      }]
    },
    {
      "description": "F2 to Cut",
      "manipulators": [{
        "from": {
          "key_code": "f2"
        },
        "to": [{
          "key_code": "x",
          "modifiers": [
            "command"
          ]
        }],
        "type": "basic"
      }]
    }, {
      "description": "F3 to Copy",
      "manipulators": [{
        "from": {
          "key_code": "f3"
        },
        "to": [{
          "key_code": "c",
          "modifiers": ["command"]
        }],
        "type": "basic"
      }]
    }, {
      "description": "F4 to Paste",
      "manipulators": [{
        "from": {
          "key_code": "f4"
        },
        "to": [{
          "key_code": "v",
          "modifiers": ["command"]
        }],
        "type": "basic"
      }]
    },
    {
      "description": "Lunch VS Code",
      "manipulators": [{
        "from": {
          "key_code": "f5"
        },
        "to": [{
          "shell_command": "open '/Applications/Visual Studio Code.app'"
        }],
        "type": "basic"
      }]
    },
    {
      "description": "Lunch Chrome",
      "manipulators": [{
        "from": {
          "key_code": "f6"
        },
        "to": [{
          "shell_command": "open '/Applications/Google Chrome.app'"
        }],
        "type": "basic"
      }]
    },
    {
      "description": "Lunch Terminal",
      "manipulators": [{
        "from": {
          "key_code": "f7"
        },
        "to": [{
          "shell_command": "open '/Applications/Utilities/Terminal.app'"
        }],
        "type": "basic"
      }]
    },
    {
      "description": "Lunch Franz",
      "manipulators": [{
        "from": {
          "key_code": "f8"
        },
        "to": [{
          "shell_command": "open '/Applications/Franz.app'"
        }],
        "type": "basic"
      }]
    }
  ]
}
