{
  "title": "Xaviers Function media Key - one hand",
  "rules": [
    {
      "description": "Shift F1 to display_brightness_decrement",
      "manipulators": [{
        "from": {
          "key_code": "f1",
          "modifiers": {
            "mandatory": ["shift"]
            }
        },
        "to": [{
          "key_code": "display_brightness_decrement"
        }],
        "type": "basic"
      }]
    },
    {
      "description": "Shift F2 to display_brightness_increment",
      "manipulators": [{
        "from": {
          "key_code": "f2",
          "modifiers": {
            "mandatory": ["shift"]
            }
        },
        "to": [{
          "key_code": "display_brightness_increment"
        }],
        "type": "basic"
      }]
    }, {
      "description": "Shift F10 to mute",
      "manipulators": [{
        "from": {
          "key_code": "f10",
          "modifiers": {
            "mandatory": ["shift"]
            }
        },
        "to": [{
          "consumer_key_code": "mute"
        }],
        "type": "basic"
      }]
    },
    {
      "description": "Shift F11 to volume_down",
      "manipulators": [{
        "from": {
          "key_code": "f11",
          "modifiers": {
            "mandatory": ["shift"]
            }
        },
        "to": [{
          "key_code": "volume_down"
                  }],
        "type": "basic"
      }]
    },
    {
      "description": "Shift F12 to volume_up",
      "manipulators": [{
        "from": {
          "key_code": "f12",
          "modifiers": {
            "mandatory": ["shift"]
            }
        },
        "to": [{
          "key_code": "volume_up"
        }],
        "type": "basic"
      }]
    }
  ]
}
